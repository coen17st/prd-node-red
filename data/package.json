{
    "name": "node-red-docker",
    "version": "1.2.9",
    "description": "Low-code programming for event-driven applications",
    "homepage": "http://nodered.org",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/node-red/node-red-docker.git"
    },
    "main": "node_modules/node-red/red/red.js",
    "scripts": {
        "start": "node $NODE_OPTIONS node_modules/node-red/red.js $FLOWS",
        "debug": "node --inspect=0.0.0.0:9229 $NODE_OPTIONS node_modules/node-red/red.js $FLOWS",
        "debug_brk": "node --inspect=0.0.0.0:9229 --inspect-brk $NODE_OPTIONS node_modules/node-red/red.js $FLOWS"
    },
    "dependencies": {
        "node-red": "1.2.9",
        "node-red-contrib-calc": "~1.0.5",
        "node-red-contrib-cast": "~0.2.16",
        "node-red-contrib-cron-plus": "~1.5.6",
        "node-red-contrib-google-tts": "1.1.1",
        "node-red-contrib-home-assistant-websocket": "~0.41.3",
        "node-red-contrib-schedex": "~1.10.6",
        "node-red-contrib-simpletime": "~2.10.0",
        "node-red-contrib-stoptimer": "0.0.7",
        "node-red-contrib-string": "1.0.0",
        "node-red-contrib-time-range-switch": "~1.1.2",
        "node-red-contrib-traffic": "~0.2.1",
        "node-red-node-rbe": "~0.5.0"
    },
    "engines": {
        "node": ">=10"
    }
}
